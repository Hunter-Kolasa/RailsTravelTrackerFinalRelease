<div class="form">
<h1>Log in</h1>
    <%= form_with(url: login_path, scope: :session, local: true) do |f| %>
      <ul>
      <%= f.label :email %>
      <%= f.email_field :email %>
      <br>
      <%= f.label :password %>
      <%= f.password_field :password %>
      <br>
      </ul>
      <%= f.submit "Log in" %>
      <%= link_to "Log in with Google", '/auth/google_oauth2', class: "button", method: :post%>
    <% end %>
</div>